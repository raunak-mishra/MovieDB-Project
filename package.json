{
  "name": "server",
  "version": "1.0.0",
  "description": "Movie Downloading Webapp In MERN Stack. React Js For Front End, Node Js And Node Js For Server And Mongo Db For Database.",
  "main": "server.js",
  "scripts": {
    "client-build": "cd client && yarn build",
    "install-client": "cd client && yarn",
    "server": "nodemon index.js",
    "client": "cd client && yarn start",
    "dev": "concurrently \"yarn server\" \"yarn client \"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false yarn install-client && yarn client-build",
    "start": "node server.js"
  },
